<h1>Songs</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
    </tr>
  </thead>
  <% @songs.each do |song| %>
    <tbody>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
      </tr>
    </tbody>
  <% end %>
</table>

<%= link_to "Add Song", new_song_path %>
