<h1>Edit Song</h1>
<%if @song.errors%>
    <ul>
        <%@song.errors.full_messages.each do |msg|%>
            <li><%=msg%></li>
        <%end%>
    </ul>
<%end%>


<%=form_for @song do |f|%>
    Title: <%= f.text_field :title %><br /><br />
    Released? : <%= f.check_box :released %><br /><br />
    Release Year : <%= f.text_field :release_year %><br /><br />
    Artist Name : <%= f.text_field :artist_name %><br /><br />
    Genre : <%= f.text_field :genre %><br /><br />

    <%=f.submit%>
<%end%>