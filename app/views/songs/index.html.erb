<h1> All Songs </h1>

<table>
    <thead>
        <tr>
            <th>Song Title</th>
            <th>Delete</th>
        </tr>
    </thead>

    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= link_to "Delete", song, method: :delete, data: { confirm: "Are your sure?" } %></td>
            </tr>
        <% end %>
    </tbody>
</table>